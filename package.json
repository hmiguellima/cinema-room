    "@types/three": "^0.133.1",
    "@webpack-cli/generators": "^2.2.0",
    "copy-webpack-plugin": "^9.1.0",
    "css-loader": "^5.2.6",
    "html-webpack-plugin": "^5.3.2",
    "prettier": "^2.3.2",
    "style-loader": "^3.0.0",
